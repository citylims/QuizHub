<div class="quiz-show">
  <h1 id="start">It's Quiz Time</h1>
  <%= link_to "Home", root_url %>

</div>

<div class="loading-indicator">
    <svg version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 130 130" overflow="visible" enable-background="new 0 0 130 130" id="progress">
      <circle fill="none" stroke-width="4" stroke-miterlimit="10" cx="65" cy="65" r="60"></circle>
      <circle class="meter" fill="none" stroke-width="4" stroke-miterlimit="10" cx="65" cy="65" r="60"></circle>
      </svg>
  </div>
<p><b class="percentage"></b></p>

<div class="interface">
  <div id="state"></div>
  <div class="controls">
    <input type="text" class="guess"></input>
    <button class="go">Start Quiz</button>
    <button class="guess-btn">Guess</button>
  </div>
</div>
